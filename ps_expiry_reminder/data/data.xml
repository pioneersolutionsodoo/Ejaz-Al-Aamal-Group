        <record id="ps_ir_cron_employee_id_contract_expiry11" model="ir.cron">
            <field name="name">Reminder Scheduler To Employee About Contract Expiry</field>
            <field name="model_id" ref="model_hr_contract"/>
            <field name="state">code</field>
            <field name="code">model.contract_expiry()</field>
            <field name="nextcall" eval="(DateTime.now() + timedelta(days=1)).strftime('%Y-%m-%d 15:40:%S')"/>
