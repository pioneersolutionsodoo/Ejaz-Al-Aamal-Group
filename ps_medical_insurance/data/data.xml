<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="seq_medical_insurance" model="ir.sequence">
            <field name="name">Medical Insurance</field>
            <field name="code">medical.insurance</field>
            <field name="prefix">MI_</field>
            <field name="padding">5</field>
        </record>

        <record id="seq_medical_insurance_upgrade" model="ir.sequence">
            <field name="name">Medical Insurance Upgrade</field>
            <field name="code">medical.insurance.upgrade</field>
            <field name="prefix">MIU_</field>
            <field name="padding">5</field>
        </record>


        <record id="medical_insurance_expiration" model="ir.cron">
            <field name="name">Check Medical Insurance Expiration</field>
            <field name="model_id" ref="model_medical_insurance"/>
            <field name="state">code</field>
            <field name="code">model.check_medical_insurance_expiration()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="doall" eval="True"/>
        </record>


    </data>
</odoo>
